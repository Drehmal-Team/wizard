; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Drehmal Installer"
config/description="The Official Installer for Drehmal 2.2 : Apotheosis, made by MoutonDemocrate"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Primordial Team/Drehmal Installer"
config/features=PackedStringArray("4.1", "Mobile")
run/max_fps=60
boot_splash/bg_color=Color(0.0862745, 0.0431373, 0.0784314, 1)
boot_splash/image="res://icon_full_length.png"
boot_splash/fullsize=false
config/icon="res://textures/pack.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Global="*res://Global.gd"
SceneTransition="*res://scene_transition.tscn"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1300
window/size/viewport_height=800
window/size/mode=2

[editor_plugins]

enabled=PackedStringArray()

[filesystem]

import/fbx/enabled=false

[physics]

common/physics_ticks_per_second=10

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
anti_aliasing/quality/msaa_2d=1
anti_aliasing/quality/msaa_3d=1
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_debanding=true
